.image_frame{
    width: 100%;
    height: 100%;
}

.section{
    background: red;
    width: 25%;
    height: 60%;
    position: absolute;
    display: -webkit-flex;
    flex-direction: column;
    flex-wrap: wrap;
    float: left;
}

.section__title{
    height: 100%;
}

.aside{
	width:5rem;
	border-right: 1px solid #ddd;
	font-size: .85rem;
	background: #f5f5f5;
	float: left;
}

.type-nav{
	position: relative;
	padding:.7rem .3rem;
	text-align: center;
	border-bottom: 1px solid #ddd;
	z-index: 10;
}

.type-nav.selected{
	margin-right: -1px;
	padding-left:-1px;
	color: #333;
	background-color: #fff;
}

